
void printf(i8* fmt, ...);
int atoi(i8* str);

int fib(int x) {
  if(x == 0) {
    return 0;
  } else if(x == 1) {
    return 1;
  } else if(x == 2) {
    return 1;
  } else {
    return fib(x-2) + fib(x-1);
  }
}

int main(int argc, i8** argv) {
  int x;
  x = atoi(argv[1]);
  printf("fib(%d) = %d", x, fib(x));
}

